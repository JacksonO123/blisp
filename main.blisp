(func create-stack (body
	(return
		(struct
			items []
			push (func _ this item (body
				(set this items (append (get this items) item))
			))
			pop (func _ this (body
				(pop (get this items))
			))
			print (func _ this (body
				(print (get this items))
			))
		)
	)
))

(var stack (create-stack))
(. stack print)
